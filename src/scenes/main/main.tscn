[sub_resource type="Resource" id="Resource_ppora"]
script = ExtResource("7_v73w7")
character = ExtResource("6_2dxjs")
text = "Thank you all for coming to this ceremony. It’s been a while. "

[sub_resource type="Resource" id="Resource_8agmq"]
script = ExtResource("7_v73w7")
character = ExtResource("6_2dxjs")
text = "Recently I had some troubles doing everything on my own. I needed to find an assistant and as you can see, there’s a new face in the crowd. Welcome, *name*!"

[sub_resource type="Resource" id="Resource_1dwxd"]
script = ExtResource("7_v73w7")
character = ExtResource("6_2dxjs")
text = "They’re going to be making today’s tea, so you’re up to criticize them for it. They’ll need to know what everyone likes"

[sub_resource type="Resource" id="Resource_bot4m"]
script = ExtResource("9_tlpgv")
lines = Array[ExtResource("7_v73w7")]([SubResource("Resource_ppora"), SubResource("Resource_8agmq"), SubResource("Resource_1dwxd")])

[sub_resource type="CanvasItemMaterial" id="CanvasItemMaterial_b8ujo"]
light_mode = 1

[sub_resource type="Resource" id="Resource_16fla"]
script = ExtResource("7_v73w7")
character = ExtResource("7_cmgwm")
text = "I am not drinking that, sorry."

[sub_resource type="Resource" id="Resource_h8lxk"]
script = ExtResource("7_v73w7")
character = ExtResource("4_4k6hw")
text = "Really? But it’s good!"

[sub_resource type="Resource" id="Resource_r0u5m"]
script = ExtResource("7_v73w7")
character = ExtResource("8_s2x27")
text = "Viv is being too harsh again."

[sub_resource type="Resource" id="Resource_5dmt7"]
script = ExtResource("7_v73w7")
character = ExtResource("7_cmgwm")
text = "I’m not harsh, I’m honest!"

[sub_resource type="Resource" id="Resource_1u0tw"]
script = ExtResource("7_v73w7")
character = ExtResource("6_2dxjs")
text = "Let’s not fight just because Viv likes plain and bitter teas."

[sub_resource type="Resource" id="Resource_rrul8"]
script = ExtResource("7_v73w7")
character = ExtResource("8_s2x27")
text = "We’re not, we’re just surprised."

[sub_resource type="Resource" id="Resource_6ls0d"]
script = ExtResource("7_v73w7")
character = ExtResource("8_eijjg")
text = "Like it’s the first time you see Viv complaining. She is honest about her tastes."

[sub_resource type="Resource" id="Resource_h4iyd"]
script = ExtResource("7_v73w7")
character = ExtResource("8_s2x27")
text = "Good point."

[sub_resource type="Resource" id="Resource_kjfxx"]
script = ExtResource("7_v73w7")
character = ExtResource("7_cmgwm")
text = "Yeah! And I’m just so special, you know. There isn't anyone with taste like mine."

[sub_resource type="Resource" id="Resource_ogryx"]
script = ExtResource("7_v73w7")
character = ExtResource("8_eijjg")
text = "Ha-ha-hah, that’s true."

[sub_resource type="Resource" id="Resource_nq2iu"]
script = ExtResource("9_tlpgv")
lines = Array[ExtResource("7_v73w7")]([SubResource("Resource_16fla"), SubResource("Resource_h8lxk"), SubResource("Resource_r0u5m"), SubResource("Resource_5dmt7"), SubResource("Resource_1u0tw"), SubResource("Resource_rrul8"), SubResource("Resource_6ls0d"), SubResource("Resource_h4iyd"), SubResource("Resource_kjfxx"), SubResource("Resource_ogryx")])

[sub_resource type="Resource" id="Resource_tfpld"]
script = ExtResource("7_v73w7")
character = ExtResource("13_rff66")
text = "How do you like this one?"

[sub_resource type="Resource" id="Resource_yxc32"]
script = ExtResource("7_v73w7")
character = ExtResource("7_cmgwm")
text = "I am going to be honest, I am not a fan. But it’s not that bad. Perhaps this just isn’t quite right for me. Even Mel has trouble making my tea perfect. It’s okay."

[sub_resource type="Resource" id="Resource_lr11b"]
script = ExtResource("7_v73w7")
character = ExtResource("8_eijjg")
text = "You have very high standards that no one can explain, Viv."

[sub_resource type="Resource" id="Resource_pgtq2"]
script = ExtResource("7_v73w7")
character = ExtResource("7_cmgwm")
text = "That I do, Om. I can’t help myself. I like what I like."

[sub_resource type="Resource" id="Resource_t7fum"]
script = ExtResource("9_tlpgv")
lines = Array[ExtResource("7_v73w7")]([SubResource("Resource_tfpld"), SubResource("Resource_yxc32"), SubResource("Resource_lr11b"), SubResource("Resource_pgtq2")])

[sub_resource type="Resource" id="Resource_fadw8"]
script = ExtResource("7_v73w7")
character = ExtResource("7_cmgwm")
text = "You know, Darlin’... This is a very good tea. I like the bitter flavor of it. "

[sub_resource type="Resource" id="Resource_nhfuw"]
script = ExtResource("7_v73w7")
character = ExtResource("7_cmgwm")
text = "Reminds me how me and my friend Anne were collecting sage by the cliffs here in Ish. "

[sub_resource type="Resource" id="Resource_i2ij4"]
script = ExtResource("7_v73w7")
character = ExtResource("7_cmgwm")
text = "I think we tried to make some kind of tea out of it, but it hadn't really worked out. It didn’t have much taste. But I do like it!"

[sub_resource type="Resource" id="Resource_32j13"]
script = ExtResource("7_v73w7")
character = ExtResource("6_2dxjs")
text = "Oh, Viv, how is Anne by the way? "

[sub_resource type="Resource" id="Resource_a64ka"]
script = ExtResource("7_v73w7")
character = ExtResource("7_cmgwm")
text = "She’s alright, she’s going to be back from Feltash soon. I am very excited for what she will bring from there this time."

[sub_resource type="Resource" id="Resource_0ooog"]
script = ExtResource("7_v73w7")
character = ExtResource("13_rff66")
text = "Who is Anne?"

[sub_resource type="Resource" id="Resource_pvwp8"]
script = ExtResource("7_v73w7")
character = ExtResource("7_cmgwm")
text = "She is a good friend of mine. She visits Torgovenitsa from time to time since she's an inventor."

[sub_resource type="Resource" id="Resource_arwlx"]
script = ExtResource("7_v73w7")
character = ExtResource("7_cmgwm")
text = "We’ve known each other for quite a long time. She always supported me through every obstacle. And always seen me for who I really am. "

[sub_resource type="Resource" id="Resource_y5j1x"]
script = ExtResource("7_v73w7")
character = ExtResource("7_cmgwm")
text = "For that I am very grateful."

[sub_resource type="Resource" id="Resource_r8bn3"]
script = ExtResource("7_v73w7")
character = ExtResource("6_2dxjs")
text = "She also goes to our ceremonies from time to time. She helped me decorate half of this place. I owe Anne a lot for that."

[sub_resource type="Resource" id="Resource_gdf0m"]
script = ExtResource("7_v73w7")
character = ExtResource("7_cmgwm")
text = "She’s very kind, you’ll meet her someday."

[sub_resource type="Resource" id="Resource_13nww"]
script = ExtResource("9_tlpgv")
lines = Array[ExtResource("7_v73w7")]([SubResource("Resource_fadw8"), SubResource("Resource_nhfuw"), SubResource("Resource_i2ij4"), SubResource("Resource_32j13"), SubResource("Resource_a64ka"), SubResource("Resource_0ooog"), SubResource("Resource_pvwp8"), SubResource("Resource_arwlx"), SubResource("Resource_y5j1x"), SubResource("Resource_r8bn3"), SubResource("Resource_gdf0m")])

[sub_resource type="Resource" id="Resource_v8qlr"]
script = ExtResource("7_v73w7")
character = ExtResource("7_cmgwm")
text = "… God, that’s too much flavor I don't like."

[sub_resource type="Resource" id="Resource_j3q0t"]
script = ExtResource("9_tlpgv")
