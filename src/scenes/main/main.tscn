[sub_resource type="Resource" id="Resource_snbeo"]
script = ExtResource("7_v73w7")
character = ExtResource("13_rff66")
text = "How’s it going... Brokk, right?"

[sub_resource type="Resource" id="Resource_w5vym"]
script = ExtResource("7_v73w7")
character = ExtResource("5_0ci1l")
text = "Oh. It’s good. Enjoying the tea."

[sub_resource type="Resource" id="Resource_sjrmn"]
script = ExtResource("7_v73w7")
character = ExtResource("5_0ci1l")
text = "Sorry for keeping quiet. I’m just very focused on tasting the flavor."

[sub_resource type="Resource" id="Resource_neie2"]
script = ExtResource("7_v73w7")
character = ExtResource("13_rff66")
text = "It’s alright. I’m glad you like it. Can you tell me something about you?"

[sub_resource type="Resource" id="Resource_nkq6i"]
script = ExtResource("7_v73w7")
character = ExtResource("5_0ci1l")
text = "Yeah, sure. Well, I’m a local smith. Trying to save some money on my dream. And I like drinking tea instead of beer."

[sub_resource type="Resource" id="Resource_r2c6w"]
