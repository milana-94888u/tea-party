text = "You like sweet flavors, right?"

[sub_resource type="Resource" id="Resource_6ireg"]
script = ExtResource("7_v73w7")
character = ExtResource("4_4k6hw")
text = "Well, if we’re talking about tea. Yes, I like sweet."

[sub_resource type="Resource" id="Resource_d1cpk"]
script = ExtResource("7_v73w7")
character = ExtResource("8_eijjg")
text = "Okay, good. Means that I still remember some stuff about you."

[sub_resource type="Resource" id="Resource_i7fuy"]
script = ExtResource("7_v73w7")
character = ExtResource("4_4k6hw")
text = "I know that you like Camomile!"

[sub_resource type="Resource" id="Resource_ji6lj"]
script = ExtResource("7_v73w7")
character = ExtResource("8_eijjg")
text = "I do!"

[sub_resource type="Resource" id="Resource_cvpos"]
script = ExtResource("7_v73w7")
character = ExtResource("4_4k6hw")
text = "Then I also remember some stuff. That’s good to know."

[sub_resource type="Resource" id="Resource_nqqop"]
