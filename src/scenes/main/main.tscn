text = "Me too, kid. Me too."

[sub_resource type="Resource" id="Resource_37hb7"]
script = ExtResource("7_v73w7")
character = ExtResource("6_2dxjs")
text = "Some tea just can’t be good for some people. It’s alright."

[sub_resource type="Resource" id="Resource_eiixu"]
script = ExtResource("7_v73w7")
character = ExtResource("7_cmgwm")
text = "I really like it, personally. I think it’s perfect."

[sub_resource type="Resource" id="Resource_gansd"]
script = ExtResource("7_v73w7")
character = ExtResource("5_0ci1l")
text = "We have extremely different tastes, Vivienne."

[sub_resource type="Resource" id="Resource_p3koj"]
script = ExtResource("7_v73w7")
character = ExtResource("7_cmgwm")
text = "Perhaps, darling. Nothing you can do about it. "

[sub_resource type="Resource" id="Resource_lqn7x"]
script = ExtResource("7_v73w7")
character = ExtResource("14_6cvn1")
text = "A strange silence howers between everyone all of a sudden. Brokk really didn’t like your tea."

[sub_resource type="Resource" id="Resource_8jttw"]
script = ExtResource("7_v73w7")
character = ExtResource("7_cmgwm")
text = "How’s the smith business going?"

[sub_resource type="Resource" id="Resource_xoy2r"]
script = ExtResource("7_v73w7")
character = ExtResource("5_0ci1l")
text = "Pretty good. I got some new equipment for making swords and some machines. Have you seen it? You can come by sometime. If you want."

[sub_resource type="Resource" id="Resource_vaxub"]
