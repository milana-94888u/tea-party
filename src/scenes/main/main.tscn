[sub_resource type="Resource" id="Resource_k4ksq"]
script = ExtResource("7_v73w7")
character = ExtResource("4_4k6hw")
text = "I don’t mind taking the cup that will be the last one left. The purple and blue one does look nice, though."

[sub_resource type="Resource" id="Resource_pd834"]
