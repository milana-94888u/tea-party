[sub_resource type="Resource" id="Resource_gdd7i"]
script = ExtResource("7_v73w7")
character = ExtResource("4_4k6hw")
text = "Could use something else, I think. But it might be just me-thing."

[sub_resource type="Resource" id="Resource_57qnr"]
